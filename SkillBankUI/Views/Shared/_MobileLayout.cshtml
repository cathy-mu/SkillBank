<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>@ViewBag.MetaTagTitle</title>
    <meta name="keywords" content="@ViewBag.MetaTagKeyWords" />
    <meta name="description" content="@ViewBag.MetaTagDescription" />
    
    <!-- Sets initial viewport load and disables zooming  -->
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">

    <!-- Makes your prototype chrome-less once bookmarked to your phone's home screen -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">

    <!-- Include the compiled Ratchet CSS -->
    <link href="/icomoon/style.css" rel="stylesheet">
    <link href="/bower_components/ratchet/dist/css/ratchet.min.css" rel="stylesheet">
    <link href="/css/common.css" rel="stylesheet">

    <!-- Include the compiled Ratchet JS -->
    <script src="/bower_components/ratchet/dist/js/ratchet.min.js"></script>

  </head>
  <body>
  @RenderBody()  
  <script src="/Scripts/mcommon.js"></script>
  </body>
</html>
